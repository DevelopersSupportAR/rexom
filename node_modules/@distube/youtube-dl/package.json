{
  "name": "@distube/youtube-dl",
  "description": "A simple Node.js wrapper for youtube-dl. Made for DisTube.js.org",
  "homepage": "https://distube.js.org",
  "version": "2.2.2",
  "main": "src/index.js",
  "author": {
    "email": "skick1234@gmail.com",
    "name": "Skick"
  },
  "contributors": [
    {
      "name": "Kiko Beats",
      "email": "josefrancisco.verdu@gmail.com"
    },
    {
      "name": "Felix Kaiser",
      "email": "felixkaiser2000@protonmail.com"
    },
    {
      "name": "Philipp Fruck",
      "email": "dev@p-fruck.de"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/distubejs/youtube-dl-exec.git"
  },
  "bugs": {
    "url": "https://github.com/distubejs/youtube-dl-exec/issues"
  },
  "keywords": [
    "binary",
    "distube",
    "distubejs",
    "nodejs",
    "youtube-dl",
    "youtube-dl-wrapper",
    "youtube-downloader"
  ],
  "dependencies": {
    "dargs": "~7.0.0",
    "execa": "~5.1.1",
    "get-stream": "~6.0.1",
    "got": "~11.8.2",
    "is-unix": "~1.0.0",
    "mkdirp": "~1.0.4",
    "p-event": "~4.2.0",
    "p-reflect": "~2.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "~12.1.4",
    "@commitlint/config-conventional": "~12.1.4",
    "ava": "latest",
    "conventional-github-releaser": "~3.1.1",
    "coveralls": "latest",
    "finepack": "latest",
    "git-authors-cli": "latest",
    "git-dirty": "latest",
    "lint-staged": "latest",
    "npm-check-updates": "~11.6.0",
    "nyc": "latest",
    "prettier-standard": "~15.0.1",
    "simple-git-hooks": "latest",
    "standard": "latest",
    "standard-markdown": "latest",
    "standard-version": "~9.3.0"
  },
  "engines": {
    "node": ">= 12"
  },
  "files": [
    "scripts",
    "src"
  ],
  "scripts": {
    "clean": "rm -rf node_modules",
    "contributors": "(git-authors-cli && finepack && git add package.json && git commit -m 'build: contributors' --no-verify) || true",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "standard-markdown README.md && standard",
    "postinstall": "node scripts/postinstall.js",
    "postrelease": "npm run release:tags && npm run release:github && npm publish",
    "prerelease": "git-dirty && npm run update:check && npm test && npm run contributors",
    "pretest": "npm run lint",
    "release": "git-authors-cli finepack && git add package.json && standard-version -a",
    "release:github": "conventional-github-releaser -p angular",
    "release:tags": "git push --follow-tags origin master",
    "test": "nyc ava",
    "update": "ncu -u",
    "update:check": "ncu -- --error-level 2"
  },
  "license": "MIT"
}
